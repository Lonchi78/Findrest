<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   tools:context=".UI.geocodeFragment" android:id="@+id/frameLayout">

    <!-- TODO: Update blank fragment layout -->
    <TextView
            android:text="Latitude"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tvLatitude"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp"/>
    <TextView
            android:text="Longitude"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tvLongitude"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/tvLatitude" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"/>
    <TextView
            android:text="Response"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tvResponse" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/tvLongitude" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp"
    />

</androidx.constraintlayout.widget.ConstraintLayout>